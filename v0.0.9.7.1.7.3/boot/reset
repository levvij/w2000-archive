#!/bin/sh

echo "resetting autogen files"
echo "//AUTOGENERATED\n// DO NOT EDIT\nconfig.injectedQuery = null;" > fs/c/windows/ime/gen/iquery.js
echo -n "" > fs/c/windows/ime/gen/iquery.prop

echo "//AUTOGENERATED\n// DO NOT EDIT\nconfig.rc = null;" > fs/c/windows/ime/gen/rc.js
echo -n "" > fs/c/windows/ime/gen/rc.prop

echo "//AUTOGENERATED\n// DO NOT EDIT\nconfig.beta = true;" > fs/c/windows/ime/gen/beta.js
echo -n true > fs/c/windows/ime/gen/beta.prop

echo "//AUTOGENERATED\n// DO NOT EDIT\nconfig.fs.providers.find(p => p.type == 'rrsp').ray = 'fs/ray.php';" > fs/c/windows/ime/gen/rrsp.js